{
  "plugins": [
    "git-tag",
    "pr-body-labels"
  ],
  "owner": "mikenabhan",
  "repo": "country-recipe-finder",
  "name": "Country Recipe Finder",
  "email": "13139061+mikenabhan@users.noreply.github.com",
  "noDefaultLabels": true,
  "labels": [
    {
      "name": "breaking",
      "changelogTitle": "💥 Breaking Change",
      "description": "Increment the major version when merged",
      "releaseType": "major",
      "color": "#C5000B"
    },
    {
      "name": "enhancement",
      "changelogTitle": "🚀 Enhancement",
      "description": "Increment the minor version when merged",
      "releaseType": "minor",
      "color": "#F1A60E"
    },
    {
      "name": "bugfix",
      "changelogTitle": "🐛 Bug Fix",
      "description": "Increment the patch version when merged",
      "releaseType": "patch",
      "color": "#870048"
    },
    {
      "name": "internal",
      "changelogTitle": "🏠 Internal",
      "description": "Changes only affect the internal API",
      "releaseType": "patch",
      "color": "#696969"
    },
    {
      "name": "dependencies",
      "changelogTitle": "🔩 Dependency Updates",
      "description": "Update one or more dependencies version",
      "releaseType": "patch",
      "color": "#8732bc"
    },
    {
      "name": "performance",
      "changelogTitle": "🏎 Performance",
      "description": "Improve performance of an existing feature",
      "releaseType": "patch",
      "color": "#f4b2d8"
    },
    {
      "name": "devops",
      "changelogTitle": "🚢 DevOps",
      "description": "Improve release pipelines, deployment, and tooling",
      "releaseType": "patch",
      "color": "#2f99a3"
    },
    {
      "name": "documentation",
      "changelogTitle": "📝 Documentation",
      "description": "Changes only affect the documentation",
      "releaseType": "patch",
      "color": "#cfd3d7"
    },
    {
      "name": "tests",
      "changelogTitle": "🧪 Tests",
      "description": "Add or improve existing tests",
      "releaseType": "none",
      "color": "#ffd3cc"
    },
    {
      "name": "skip-release",
      "description": "Preserve the current version when merged",
      "releaseType": "skip",
      "color": "#bf5416"
    }
  ]
}
